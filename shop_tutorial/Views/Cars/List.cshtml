<!DOCTYPE html>


<h2>Все автомобили</h2>
<h1>Текст из ViewBag: @Model.CurrCategory</h1>
<div class="row mt-5 mb-2">
    @{
        foreach (var car in Model.AllCars)
        {
            <div class="col-lg-4">
                <h2>Модель: @car.Name</h2>
                <p>
                    Цена: @car.Price.ToString("c")
                </p>
                <p>
                    <a class="btn btn-warning" href="#">Подробнее</a>
                </p>
            </div>
        }
    }
</div>

